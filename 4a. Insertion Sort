import random
def gen_data(a,n): 
for i in range(n):
x=random.randint(0,100) 
a.append(x)
def ins_sort(a):
for i in range(1,len(a)): 
key=a[i]
j=i-1
while j>=0 and key<a[j]: 
a[j+1]=a[j]
j=j-1 
a[j+1]=key


a=[ ]
print("enter size: ") 
n=int(input()) 
gen_data(a,n)
print(" the numbers are: ") 
print (a)
ins_sort(a)
print(" the sorted numbers are: ") 
print (a)
